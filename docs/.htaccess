RewriteEngine on
RewriteCond %{HTTP_HOST} ^(www.)?tyxz.github.io/PixelTransfer$
RewriteCond %{REQUEST_URI} !^/html/
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ /html/$1
RewriteCond %{HTTP_HOST} ^(www.)?tyxz.github.io/PixelTransfer$
RewriteRule ^(/)?$ html/index.html [L] 